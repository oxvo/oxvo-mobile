{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "preserve",
    "strict": true,
    "paths": {
      "@oxvo-mobile/assets/*": ["./app/assets/*"],
      "@oxvo-mobile/components/*": ["./app/components/*"],
      "@oxvo-mobile/config/*": ["./app/config/*"],
      "@oxvo-mobile/features/*": ["./app/features/*"],
      "@oxvo-mobile/constants/*": ["./app/constants/*"],
      "@oxvo-mobile/hooks/*": ["./app/hooks/*"],
      "@oxvo-mobile/libs/*": ["./app/libs/*"],
      "@oxvo-mobile/mocks/*": ["./app/mocks/*"],
      "@oxvo-mobile/navigation/*": ["./app/navigation/*"],
      "@oxvo-mobile/screens/*": ["./app/screens/*"],
      "@oxvo-mobile/store/*": ["./app/store/*"],
      "@oxvo-mobile/utils/*": ["./app/utils/*"]
    }
  }
}
